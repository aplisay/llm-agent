  "version": "0.2.0",
  "configurations": [
    

    {
      "command": "yarn test",
      "name": "Run yarn test",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "cwd": "${workspaceFolder}/agents/jambonz",
      "command": "yarn develop",
      "name": "Run jambonz agent",
      "request": "launch",
      "type": "node-terminal"
    }
    
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}