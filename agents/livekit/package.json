{
  "name": "livekit-agent",
  "version": "0.9.1",
  "description": "Omni provider voice agent",
  "main": "dist/realtime.js",
  "exports": {
    ".": {
      "import": "./dist/realtime.js",
      "require": "./dist/realtime.js"
    },
    "./lib/*": {
      "import": "./dist/lib/*.js",
      "require": "./dist/lib/*.js"
    }
  },
  "scripts": {
    "develop": "yarn build && NODE_ENV=development LOGLEVEL=debug nodemon -e js,mjs,json,yaml,ts dist/realtime.js dev",
    "start": "yarn build && NODE_ENV=production node dist/realtime.js start",
    "stage": "yarn build && NODE_ENV=staging LOGLEVEL=debug node dist/realtime.js dev",
    "build": "tsc && yarn postbuild",
    "build:full": "tsc && yarn postbuild",
    "postbuild": "./fix-imports.sh",
    "dev": "tsc --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/pino-logging-gcp-config": "^1.0.4",
    "@livekit/agents": "0.7.9",
    "@livekit/agents-plugin-openai": "^0.9.1",
    "@livekit/rtc-node": "^0.13.11",
    "async-mutex": "^0.4.1",
    "axios": "^1.9.0",
    "command-line-args": "^6.0.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "github:rjp44/secretenv",
    "livekit-server-sdk": "^2.13.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "pg-listen": "^1.7.0",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "sequelize": "^6.37.7",
    "uuid": "^9.0.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/ws": "^8.18.1",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "tsup": "^8.0.2",
    "typescript": "^5.8.3"
  }
}
