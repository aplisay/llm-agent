{
  "name": "llm-agent",
  "version": "0.9.1",
  "description": "Omni provider voice agent",
  "type": "module",
  "main": "index.mjs",
  "scripts": {
    "develop": "NODE_ENV=development LOGLEVEL=debug nodemon -e js,mjs,json,yaml index.mjs",
    "start": "NODE_ENV=production node index.mjs",
    "stage": "NODE_ENV=staging LOGLEVEL=debug node index.mjs",
    "test": "LOGLEVEL=fatal node --experimental-vm-modules node_modules/.bin/jest --config jest.config.db.js",
    "test:db": "docker-compose -f tests/docker-compose.test.yml up -d && yarn test && docker-compose -f tests/docker-compose.test.yml down -v",
    "test:no-db": "LOGLEVEL=fatal node --experimental-vm-modules node_modules/.bin/jest --config jest.config.no-db.js",
    "test:ci": "docker-compose -f docker-compose.ci.yml build --no-cache test-runner && docker-compose -f docker-compose.ci.yml up --abort-on-container-exit --exit-code-from test-runner",
    "test:cleanup": "docker-compose -f tests/docker-compose.test.yml down -v",
    "livekit-dev": "NODE_ENV=development LOGLEVEL=debug nodemon -e js,mjs,json,yaml agents/livekit/realtime.mjs dev"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@deepgram/sdk": "^3.13.0",
    "@google-cloud/pino-logging-gcp-config": "^1.0.4",
    "@google-cloud/text-to-speech": "^4.2.3",
    "@google-cloud/vertexai": "^0.5.0",
    "@jambonz/node-client-ws": "github:rjp44/node-client-ws#main",
    "@livekit/rtc-node": "^0.13.12",
    "async-mutex": "^0.4.1",
    "axios": "^1.9.0",
    "command-line-args": "^6.0.1",
    "config": "^3.3.12",
    "cors": "^2.8.5",
    "dotenv": "github:rjp44/secretenv#v1.0.5",
    "elevenlabs": "^0.4.1",
    "express": "^4.21.2",
    "express-openapi": "^12.1.3",
    "js-yaml": "^4.1.0",
    "livekit-server-sdk": "^2.13.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.0",
    "pg-hstore": "^2.3.4",
    "pg-listen": "^1.7.0",
    "pino": "^9.7.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "sequelize": "^6.37.7",
    "swagger-sequelize": "^0.1.5",
    "uuid": "^9.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "gulp": "^4.0.2",
    "gulp-exec": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "jest": "^30.2.0",
    "jsdoc-to-markdown": "^8.0.3",
    "nodemon": "^2.0.22",
    "supertest": "^7.1.4",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "firebase-admin": "^11.11.1"
  }
}
